<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" prefHeight="461.0" prefWidth="445.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.casinelli.Appointments.Controller.LoginController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <children>
      <AnchorPane prefHeight="435.0" prefWidth="443.0">
         <children>
            <Button fx:id="buttonLogin" accessibleText="Login Button" alignment="BOTTOM_CENTER" defaultButton="true" layoutX="180.0" layoutY="276.0" onAction="#onLoginClick" text="Login" />
            <TextField fx:id="tfUsername" layoutX="129.0" layoutY="127.0" promptText="Enter Username" />
            <PasswordField fx:id="tfPassword" layoutX="129.0" layoutY="198.0" promptText="Enter Password" />
         </children>
      </AnchorPane>
   </children>
</VBox>
